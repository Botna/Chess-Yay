<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <!--  <item name="android:listPopupWindowStyle">@style/MyListPopupWindow</item>-->
    </style>
    
   <style name="MyActionBar" parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
       
	</style>
     <!--  popupList  -->	
   <style name="MyPopupListStyle" parent="@style/Widget.AppCompat.Light.ActionBar.Solid">  
        <item name="android:background">#FFFFFF</item>
   </style>
        
       <style name="MyActionBarTabText" parent="@style/Widget.AppCompat.ActionBar.TabText"></style>
		<style name="MyActionBarTitleText" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title"></style>
</resources>
